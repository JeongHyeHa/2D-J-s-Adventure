# This .gitignore file should be placed at the root of your Unity project directory

# Ignore hidden folders and files
.DS_Store
Thumbs.db
Desktop.ini

# User-specific files
*.swp
*.swo
*.user
*.userprefs

# Autogenerated files and folders
Library/
Temp/
Obj/
Build/
Builds/
Logs/
MemoryCaptures/
Packages/
ProjectSettings/ProjectVersion.txt
ProjectSettings/EditorBuildSettings.asset
ProjectSettings/QualitySettings.asset
ProjectSettings/ProjectSettings.asset
Recordings/*.mp4

# Visual Studio / MonoDevelop project files
.vscode/
*.csproj
*.unityproj
*.sln
*.suo
*.tmp
*.user
*.userprefs
*.pidb
*.booproj
*.svd

# Unity specific
*.pidb.meta
*.pdb
*.mdb
*.opendb
*.VC.db
.meta

# Ignore crash reports, log files, temporary files, and builds
*.stackdump
*.log
*.tmp
*.temp
*.blend1

# Ignore Visual Studio folder
.vs/

# Ignore Unity generated metadata files
*.pidb.meta
